{
    "name": "faryshta/yii2-clockpicker",
    "description": "Yii2 wrapper for clockpicker (clock-like time picker) plugin",
    "keywords": [
        "yii",
        "yii2",
        "clockpicker",
        "jquery",
        "timepicker"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/MadAnd/yii2-clockpicker",
    "authors": [
        {
            "name": "Andriy Kmit'",
            "email": "dev@madand.net",
            "homepage": "https://github.com/MadAnd",
            "role": "Developer"
        },
        {
            "name": "Angel (Faryshta) Guevara",
            "email": "angeldelcaos@gmail.com",
            "homepage": "https://github.com/Faryshta",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "bower-asset/clockpicker": "*"
    },
    "autoload": {
        "psr-4": {
            "faryshta\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
    }
}
