{
    "name": "madand/yii2-clockpicker",
    "description": "Yii2 wrapper for clockpicker (clock-like time picker) plugin",
    "keywords": [
        "yii",
        "yii2",
        "clockpicker"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/MadAnd/yii2-clockpicker",
    "authors": [
        {
            "name": "Andriy Kmit'",
            "email": "dev@madand.net",
            "homepage": "https://github.com/MadAnd",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "yiisoft/yii2-bootstrap": "*",
        "bower-asset/clockpicker": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "madand\\clockpicker\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
        "asset-repositories": {
            "bower-asset/clockpicker": {
                "type": "bower-vcs",
                "url": "https://github.com/MadAnd/clockpicker"
            }
        }
    }
}
